  "name": "object-descriptors",
  "version": "0.0.0",
  "description": "",
  "cdn": "dist/object-descriptors.js",
  "main": "dist/object-descriptors.cjs.js",
  "types": "index.d.ts",
  "unpkg": "dist/object-descriptors.js",
  "module": "dist/object-descriptors.es.js",
  "jsdelivr": "dist/object-descriptors.js",
  "umd:main": "dist/object-descriptors.js",
  "files": [
    "dist/",
    "index.d.ts"
  ],
  "scripts": {
    "test:build": "tsc index.spec.ts --lib es2018 --target es2015 --skipLibCheck --moduleResolution node",
    "pretest": "yarn build && yarn test:build",
    "test": "ava index.spec.js --verbose --color",
    "build:typescript": "tsc index.ts --lib es2018 --target es2015 --strict --declaration",
    "build:bili": "bili index.js --format es,cjs,umd --banner --js buble",
    "build": "yarn build:typescript && yarn build:bili"
  },
